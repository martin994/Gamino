<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ListaDeIntereses">


    <TextView
        android:id="@+id/interesTV"
        android:layout_width="match_parent"
        android:layout_height="37dp"
        android:text="@string/ListaIntereses" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/InteresesRV"
        android:layout_width="match_parent"
        android:layout_height="422dp" />

    <Button
        android:id="@+id/InteresesBtnConfirmar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/InteresesConfirmar" />

    <Button
        android:id="@+id/InteresesBtnOmitir"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/InteresesOmitir" />

</LinearLayout>